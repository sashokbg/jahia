<?xml version="1.0" encoding="UTF-8"?>
<content xmlns:j="http://www.jahia.org/jahia/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0">
    <modules jcr:primaryType="jnt:modules">
        <big-test-templates j:dependencies="default"
                          j:installedModules="big-test-templates"
                          j:modulePriority="0"
                          j:moduleType="templatesSet"
                          j:title="Big Test Templates"
                          jcr:primaryType="jnt:module">

            <portlets jcr:primaryType="jnt:portletFolder"/>
            <files jcr:primaryType="jnt:folder"/>
            <contents jcr:primaryType="jnt:contentFolder"/>
            <templates jcr:primaryType="jnt:templatesFolder">
                <files jcr:primaryType="jnt:folder">
                    <themes jcr:primaryType="jnt:folder">
                            <default jcr:primaryType="jnt:folder"/>
                    </themes>
                        <settings.png j:height="16"
                             j:width="16"
                             jcr:mixinTypes="jmix:image"
                             jcr:primaryType="jnt:file">
                            <jcr:content jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail2 jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                        </settings.png>
                </files>
                <contents jcr:primaryType="jnt:contentFolder"/>
                    <base j:view="big-test-templates" jcr:primaryType="jnt:template">
                        <home j:themeName="default"
                     j:view="big-test-templates"
                     jcr:mixinTypes="jmix:theme"
                     jcr:primaryType="jnt:pageTemplate"/>
                        <simple j:view="big-test-templates" jcr:primaryType="jnt:pageTemplate">
                        <pagecontent jcr:primaryType="jnt:contentList"/>
                        </simple>
                        <pagecontent jcr:primaryType="jnt:contentList"/>
                </base>
                    <content-template j:applyOn="jnt:content"
                              j:hiddenTemplate="true"
                              j:priority="1"
                              j:view="big-test-templates"
                              jcr:primaryType="jnt:contentTemplate">
                    <pagecontent jcr:primaryType="jnt:contentList">
                            <main-resource-display j:mainResourceView="default" jcr:primaryType="jnt:mainResourceDisplay"/>
                    </pagecontent>
                </content-template>
                    <site-settings-base j:view="siteSettings"
                                jcr:mixinTypes="jmix:useUILocale"
                                jcr:primaryType="jnt:template">
                        <customize-big-test-template j:applyOn="jnt:virtualsite"
                                            j:defaultTemplate="false"
                                            j:hiddenTemplate="false"
                                            j:icon="/modules/big-test-templates/templates/files/settings.png"
                                            j:view="siteSettings"
                                            jcr:primaryType="jnt:contentTemplate">
                            <j:translation_fr jcr:language="fr"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="Customize Big Test Template"/>
                            <j:translation_de jcr:language="de"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="Customize Big Test Template"/>
                            <j:translation_pt jcr:language="pt"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="Customize Big Test Template"/>
                            <j:translation_en jcr:language="en"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="Customize Big Test Template"/>
                            <j:translation_it jcr:language="it"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="Customize Big Test Template"/>
                            <j:translation_es jcr:language="es"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="Customize Big Test Template"/>
                            <pagecontent jcr:primaryType="jnt:contentList">
                                <big-test-site-customization jcr:primaryType="jnt:bigTestTemplateCustomization">
                                    <j:translation_fr jcr:language="fr"
                                          jcr:mixinTypes="mix:title"
                                          jcr:primaryType="jnt:translation"
                                          jcr:title="Big Test Site Customization"/>
                                    <j:translation_de jcr:language="de"
                                          jcr:mixinTypes="mix:title"
                                          jcr:primaryType="jnt:translation"
                                          jcr:title="Big Test Site Customization"/>
                                    <j:translation_pt jcr:language="pt"
                                          jcr:mixinTypes="mix:title"
                                          jcr:primaryType="jnt:translation"
                                          jcr:title="Big Test Site Customization"/>
                                    <j:translation_en jcr:language="en"
                                          jcr:mixinTypes="mix:title"
                                          jcr:primaryType="jnt:translation"
                                          jcr:title="Big Test Site Customization"/>
                                    <j:translation_it jcr:language="it"
                                          jcr:mixinTypes="mix:title"
                                          jcr:primaryType="jnt:translation"
                                          jcr:title="Big Test Site Customization"/>
                                    <j:translation_es jcr:language="es"
                                          jcr:mixinTypes="mix:title"
                                          jcr:primaryType="jnt:translation"
                                          jcr:title="Big Test Site Customization"/>
                                </big-test-site-customization>
                            </pagecontent>
                        </customize-big-test-template>
                        <pagecontent jcr:primaryType="jnt:contentList"/>
                    </site-settings-base>
                    <server-settings-base jcr:mixinTypes="jmix:useUILocale" jcr:primaryType="jnt:template">
                        <pagecontent jcr:primaryType="jnt:contentList"/>
                        <customize-big-test-server j:applyOn="jnt:globalSettings"
                                        j:defaultTemplate="false"
                                        j:hiddenTemplate="false"
                                        j:icon="/modules/big-test-templates/templates/files/settings.png"
                                          j:view="serverSettings"
                                        jcr:primaryType="jnt:contentTemplate">
                            <j:translation_fr jcr:language="fr"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="Customize Big Test Server"/>
                            <pagecontent jcr:primaryType="jnt:contentList">
                                <customize-big-test-server jcr:primaryType="jnt:bigTestTemplateCustomization">
                                    <j:translation_fr jcr:language="fr"
                                          jcr:mixinTypes="mix:title"
                                          jcr:primaryType="jnt:translation"
                                          jcr:title="Customize Big Test Server"/>
                                    <j:translation_de jcr:language="de"
                                          jcr:mixinTypes="mix:title"
                                          jcr:primaryType="jnt:translation"
                                          jcr:title="Customize Big Test Server"/>
                                    <j:translation_pt jcr:language="pt"
                                          jcr:mixinTypes="mix:title"
                                          jcr:primaryType="jnt:translation"
                                          jcr:title="Customize Big Test Server"/>
                                    <j:translation_en jcr:language="en"
                                          jcr:mixinTypes="mix:title"
                                          jcr:primaryType="jnt:translation"
                                          jcr:title="Customize Big Test Server"/>
                                    <j:translation_it jcr:language="it"
                                          jcr:mixinTypes="mix:title"
                                          jcr:primaryType="jnt:translation"
                                          jcr:title="Customize Big Test Server"/>
                                    <j:translation_es jcr:language="es"
                                          jcr:mixinTypes="mix:title"
                                          jcr:primaryType="jnt:translation"
                                          jcr:title="Customize Big Test Server"/>
                                </customize-big-test-server>
                            </pagecontent>
                        </customize-big-test-server>
                    </server-settings-base>
            </templates>
                
                <home j:isHomePage="true"
               j:templateName="home"
               jcr:primaryType="jnt:page">
                    <j:translation_en jcr:language="en"
                              jcr:mixinTypes="mix:title"
                              jcr:primaryType="jnt:translation"
                              jcr:title="Home"/>
                </home>
        </big-test-templates>
    </modules>
</content>
