//////////////////////////////////////////////////////////////////////////
//   PARTIE USERS  //////////////////////////////////////////////////
//////////////////////////////////////////////////////////////////////////

// Formulaire d'inscription : 
[jnt:ciInscriptionForm] > jnt:content, jmix:ciUserComponents, jnt:ciMail
 - title (string) = 'inscription'
 - redirectPage (weakreference, picker[type='page']) mandatory
 - cguPage (weakreference, picker[type='page']) mandatory

// Confirmation de l'inscription :
[jnt:ciInscriptionConfirmation] > jnt:content, jmix:ciUserComponents
  - contentFRUser (string, richtext) mandatory
  - contentRJUser (string, richtext) mandatory

[jnt:ciUserActivities] > jnt:content, jmix:ciUserComponents
  - title (string)

[jnt:ciUserProfil] > jnt:content, jmix:ciUserComponents
  - title (string)

[jnt:ciMemberProfile] > jnt:content, jmix:ciUserComponents
  - title (string)

[jnt:ciProfessionalProfile] > jnt:content, jmix:ciUserComponents
  - title (string)

[jnt:ciModeratorProfile] > jnt:content, jmix:ciUserComponents
  - title (string)

// Formulaire de desinscription : 
[jnt:ciUnsubscribeForm] > jmix:ciUserComponents
 - title (string)
 
//////////////////////////////////////////////////////////////
//   BOXES  //////////////////////////////////////////////////
//////////////////////////////////////////////////////////////
// Box de connexion :
[jnt:ciConnectionBox] > jnt:content, jmix:ciBoxComponents, jnt:ciMail
 - blocTitle (string) hidden
 - blocSecondTitle (string) hidden
 - leftTabTitle (string) hidden
 - leftTabContent (string, richtext) hidden
 - rightTabTitle (string) hidden
 - rightTabContent (string, richtext) hidden
 - tabHeight (long) hidden
 - switchTabs (string) hidden 
 - moderatorMail (string)  hidden



// Box de remontee de question :
[jnt:ciLastQuestionsBox] > jnt:content, jmix:ciBoxComponents
 - boxTitle (string)
 - nbHit (long) = 3 mandatory < '[2,10]'

// Box lien unique :
[jnt:ciOneLinkBox] > jnt:content, jmix:ciBoxComponents
 - boxTitle (string) mandatory
 - boxTitle2 (string)
 - boxTitle3 (string)
 - link (weakreference, picker[type='page'])
 
// Box Les utilisateurs (les professionels, les membres) :
[jnt:ciUsersBox] > jnt:content, jmix:ciBoxComponents, jmix:renderable
 - boxTitle (string)
 - boxMostActiveTitle (string)
 - boxType (string, choicelist[resourceBundle]) = 'actifs' < 'actifs','members','professionals'

// Box des questions de la meme rubrique:
[jnt:ciRubricQuestionsBox] > jnt:content, jmix:ciBoxComponents
 - boxTitle (string)
 
// Box des articles les plus vues de la rubrique :
[jnt:ciRubricArticlesBox] > jnt:content, jmix:ciBoxComponents
 - boxTitle (string)

// Box Actualite des mes thematiques :
[jnt:ciUserRubricNewsBox] > jnt:content, jmix:ciBoxComponents
 - boxTitle (string)
 - nbOfQuestions (long) = 2 autocreated
 - nbOfArticles (long) = 2 autocreated

[jnt:ciRelatedForumItem] > jmix:ciBoxComponents
 - forumPage (weakreference, picker[type='page'])

// Box des forum en relation (contribuable) :
[jnt:ciRelatedForumsBox] > jnt:content, jmix:ciBoxComponents, jmix:list, jmix:renderableList
 orderable
 - boxTitle (string)
 + * (jnt:ciRelatedForumItem) = jnt:ciRelatedForumItem

//Newsletter
[jnt:ciNewsletterSubscriptionBox] > jnt:content, jmix:ciBoxComponents, mix:title
  
[jnt:ciSubscriptionComponent] > jnt:content, jmix:autoPublish
  - email (string) hidden
  
/////////////////////////////////////////////////////////////////////
//   PARTIE FORUM  //////////////////////////////////////////////////
/////////////////////////////////////////////////////////////////////

// Forum Retraite : 
[jnt:ciReply] > jnt:content, jmix:ciPickable, jmix:ciRecommandable
 - title (string) boost=2.0
 - body (string, richtext) boost=1.5
 - user (weakreference, picker[type='user'])

[jnt:ciQuestion] > jnt:content, jmix:ciForumComponents, jmix:ciViewCountable, jmix:ciSearchable, jmix:ciPickable
 - pageTitle (string)
 - title (string) primary boost=2.0
 - body (string, richtext) boost=1.5
 - user (weakreference, picker[type='user'])
 - modifiedDate (date)
 - nbOfReplies (long) =0 autocreated hidden
 - nbOfProfReplies (long) =0 autocreated hidden
 + * (jnt:ciReply)

// Forumlaire de création de question :
[jnt:ciQuestionForm] > jnt:content, jmix:ciForumComponents
 - title (string)
 - summary (string) hidden
 - underTitle1 (string)
 - underTitle2 (string)
 + * (jnt:ciLink) = jnt:ciLink hidden
 - link1Label (string) hidden
 - link1Ref (string) hidden
 - link2Label (string) hidden
 - link2Ref (string) hidden
 - body (string, richtext)

// Liste des questions :
[jnt:ciQuestionList] > jnt:content, jmix:ciForumComponents
 - title (string)

// Liste des membres
[jnt:ciMemberList] >jnt:content, jmix:ciForumComponents
 - title(string)

// Liste des professionnels
[jnt:ciProList] >jnt:content, jmix:ciForumComponents
 - title(string)

//////////////////////////////////////////////////////////////////////////
//   PARTIE EDITORIALE  //////////////////////////////////////////////////
//////////////////////////////////////////////////////////////////////////

[jnt:ciTabularList] > jnt:content, jmix:ciArticleComponents
 orderable
 + * (jmix:ciArticleComponents) = jmix:ciArticleComponents version

[jnt:ciArticleBody] > jnt:content, jmix:ciArticleComponents, jmix:ciSearchable
 - tabTitle1 (string) boost=0.5
 - tabTitle2 (string) boost=0.5
 - tabTitle3 (string) boost=0.5
 - body (string,richtext) boost=1.5
 - marker (string)
 
[jnt:ciArticlePagination]  > jnt:content, jmix:ciArticleComponents
 + * (jmix:ciArticleComponents) = jmix:ciArticleComponents version

[jnt:ciArticle] > jnt:content, jmix:ciEditorialComponents, jmix:ciViewCountable, jmix:ciSearchable, jmix:list, jmix:ciRecommandable
 - title (string) boost=2.0
 - intro (string, richtext) boost=0.5
 - introImage (weakreference, picker[type='image'])
 - sliderImage (weakreference, picker[type='image'])
 - homeSliderImage (weakreference, picker[type='image'])
 - enforcedDate (date)
 + * (jmix:ciArticleComponents) = jmix:ciArticleComponents version

[jnt:ciLireAussi] > jnt:content, jmix:ciArticleComponents, jmix:nodeReference
 - title (string)
 - body (string,richtext)
 - linkmarker (string)
 - displayStyle (string, choicelist[resourceBundle]) = 'publiRedactionnel' < 'publiRedactionnel','boxLireAussi'
 
// Liste des articles (page thematique)
[jnt:ciArticlesList] > jnt:content, jmix:ciEditorialComponents
 - title (string)
 
[jnt:ciArticlesSliderItem] > jnt:content, jmix:ciEditorialComponents
 - pageArticle (weakreference, picker[type='page']) mandatory
 
// Slider articles :
[jnt:ciArticlesSlider] > jnt:content, jmix:ciEditorialComponents, jmix:list, jmix:renderableList
 orderable
 - nbOfArticles (long) = 3 autocreated 
 + * (jnt:ciArticlesSliderItem)

 
/////////////////////////////////////////////////////////////
// MAILS CONFIGURATION //////////////////////////////////////
/////////////////////////////////////////////////////////////

[jnt:ciMailConfigSimple] > jnt:content, jmix:ciConfigComponents
 - jcr:title (string) nofulltext i18n mandatory
 - subject (string) nofulltext
 - recipientMailBody (string, richtext) nofulltext mandatory

[jnt:ciMailConfigWithSender] > jnt:ciMailConfigSimple
 - senderMailBody (string, richtext) nofulltext mandatory

 
