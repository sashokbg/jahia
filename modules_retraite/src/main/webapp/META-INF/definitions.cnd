<jnt = 'http://www.jahia.org/jahia/nt/1.0'>
<jmix = 'http://www.jahia.org/jahia/mix/1.0'>



//////////////////////////////////////////////////////////////////////////
// CUSTOM MIXIN TYPES ////////////////////////////////////////////////////
//////////////////////////////////////////////////////////////////////////

[jmix:ciComponents] > jmix:droppableContent, jmix:cache mixin

[jmix:ciConfigComponents] > jmix:droppableContent, mix:title mixin

[jmix:ciUserComponents] > jmix:droppableContent mixin

[jmix:ciForumComponents] > jmix:droppableContent mixin

[jmix:ciEditorialComponents] > jmix:droppableContent mixin

[jmix:ciBoxComponents] > jmix:droppableContent mixin

[jmix:ciNavigationComponents] > jmix:droppableContent mixin

[jmix:ciHomeComponents] > jmix:droppableContent mixin

[jmix:ciArticleComponents] > jmix:ciEditorialComponents mixin

[jmix:ciPickable] mixin
 - isNotPickable (boolean) = false autocreated

[jmix:ciToolsPickable] mixin
 - isNotToolsDisplayed (boolean) = false autocreated

[jmix:ciRated] mixin
 - score (long) = 0 autocreated hidden onconflict=sum

[jmix:ciViewCountable] mixin
 - nbOfViews (long) = 0  autocreated hidden onconflict=sum
 
[jmix:ciSearchable] mixin // to be searchable in site

[jmix:ciRecommandable] mixin
 - recommandedScore (long) = 0 autocreated hidden onconflict=sum
 
//////////////////////////////////////////////////////////////////////////
// SITE EXTENSIONS ///////////////////////////////////////////////////////
//////////////////////////////////////////////////////////////////////////
 
//definition generales du site
[jmix:ciSiteConfiguration] mixin 
 extends = jnt:virtualsite
 itemtype = options
 - moderatorToolsPageLink (weakreference, picker[type='page'])
 - contactPageLink (weakreference, picker[type='page'])
 - searchPageLink (weakreference, picker[type='page'])
 - inscriptionPageLink (weakreference, picker[type='page'])
 - connectionProtocol (string, choicelist[resourceBundle]) = 'http' < 'http','https'
 - favicon (weakreference, picker[type='file'])

[jmix:ciGaConfiguration] mixin 
 extends = jnt:virtualsite
 itemtype = options
 - gaAccountID (string) = 'UA-29661433-18' nofulltext indexed='no' < '\\bUA-\\d{4,10}-\\d{1,4}\\b'
 - isGaDev (boolean) = false
 - isGaTagCanonical (boolean) = false
 - signatureTagPro (string)

//definition des configurations mail 
[jmix:ciMailConfiguration] mixin
extends = jnt:virtualsite
itemtype = options
 - notificationActivated (boolean) = false
 - siteAddress (string)
 - gaSuffixExperts (string)
 - gaSuffixMembres (string)
 - mailTemplateLogo (weakreference, picker[type='file'])
 - mailTemplateLine (weakreference, picker[type='file'])
 - mailTemplateBorder (weakreference, picker[type='file'])
 - emailNoReply (string) 
 - emailFrom (string)
 - emailTo(string)
 - welcomeTarget				(weakreference,choicelist[nodes='$currentSite//*;jnt:ciMailConfigSimple'])		< jnt:ciMailConfigSimple
 - messageMemberTarget  		(weakreference,choicelist[nodes='$currentSite//*;jnt:ciMailConfigWithSender'])	< jnt:ciMailConfigWithSender
 - changePasswordTarget			(weakreference,choicelist[nodes='$currentSite//*;jnt:ciMailConfigSimple'])		< jnt:ciMailConfigSimple
 - replyNotificationTarget		(weakreference,choicelist[nodes='$currentSite//*;jnt:ciMailConfigSimple'])		< jnt:ciMailConfigSimple
 - reportTarget					(weakreference,choicelist[nodes='$currentSite//*;jnt:ciMailConfigSimple'])		< jnt:ciMailConfigSimple
 - messageFriendlyMemberArticleTarget (weakreference,choicelist[nodes='$currentSite//*;jnt:ciMailConfigSimple']) < jnt:ciMailConfigSimple
 - messageFriendlyMemberQuestionTarget (weakreference,choicelist[nodes='$currentSite//*;jnt:ciMailConfigSimple'])	< jnt:ciMailConfigSimple
 - reportMailTitle(string) nofulltext hidden
 - reportMailBody (string, textarea) nofulltext hidden
 - replyMailTitle(string) nofulltext hidden
 - replyMailBody (string, textarea) nofulltext hidden
 - changePasswordMailTitle (string) nofulltext hidden
 - changePasswordMailBody (string, textarea) nofulltext hidden
 - messageMemberMailTitle(string) nofulltext hidden
 - messageMemberRecipientMailBody (string, textarea) nofulltext hidden
 - messageMemberSenderMailBody (string, textarea) nofulltext hidden
 - welcomeSubject (string) nofulltext hidden
 - welcomebody (string, textarea) nofulltext hidden


//Mail de notification :
[jnt:ciMail] > jmix:ciComponents mixin
 - from (string) hidden
 - cci (string)
 - subject (string) protected
 - body (string, richtext) hidden


// Box de connexion :
[jmix:ciConnectionBoxConfiguration] mixin
extends = jnt:virtualsite
itemtype = options
 - blocTitle (string) mandatory
 - blocSecondTitle (string) mandatory
 - leftTabTitle (string) mandatory
 - leftTabContent (string, textarea) mandatory
 - rightTabTitle (string) mandatory
 - rightTabContent (string, textarea) mandatory
 - tabHeight (long) = 110
 - switchTabs (string, choicelist[resourceBundle]) = 'click' < 'mouseMove','click'


//definition du nombre d'item pour la pagination
[jmix:ciPaginateConfiguration] mixin
extends = jnt:virtualsite
itemtype = options
 - paginationmaxItems(long) = 6
 - paginationmaxForumQuestionList(long) = 10
 - paginationmaxQuestionList(long) = 6
 - paginationMaxResearchList(long) = 8
 - paginationProMaxReplyList(long) = 4
 
//definition de l'ordre par defaut des listes
[jmix:ciOrderConfiguration] mixin
extends = jnt:virtualsite
itemtype = options
 - questionsListOrder (string, choicelist[resourceBundle]) = 'lastmod' < 'lastmod','view', 'active'
 - repliesListOrder (string, choicelist[resourceBundle]) = 'score' < 'lastmod','score'
 - addQuestionLink (weakreference, picker[type='page'])


//community users management options
[jmix:ciUsersManagement] mixin
extends = jnt:virtualsite
itemtype = options
 - avatarMaxSize (long) = 6291456 mandatory autocreated
 - userProfilLink (weakreference, picker[type='page'])
 - userUnsubscribeLink (weakreference, picker[type='page'])
 - userActivitiesLink (weakreference, picker[type='page'])
 - memberProfileLink (weakreference, picker[type='page'])
 - membersListLink (weakreference, picker[type='page'])
 - professionalProfileLink (weakreference, picker[type='page'])
 - professionalsListLink (weakreference, picker[type='page'])
 - moderatorProfileLink (weakreference, picker[type='page'])

//member listing options
[jmix:ciuserListConfiguration] mixin
extends = jnt:virtualsite
itemtype = options
 - memberListLineperPage(long) = 3
 - proListLineperPage(long) = 3
 - usersBoxListOrder (string, choicelist[resourceBundle]) = 'createdDate' < 'createdDate','mostActive'

//maintenance mode configuration
[jmix:ciMaintenanceConfiguration] mixin
 extends = jnt:virtualsite
 itemtype = options
// - redirectionPageLink (weakreference, picker[type='page']) nofulltext
 - onMaintenance (boolean) = false nofulltext 

// facebook admin and default conf
[jmix:ciFbConfiguration] mixin
 extends = jnt:virtualsite
 itemtype = options
 - fb_admins (string) nofulltext
 - fb_app_id (string) nofulltext
 - og_image (weakreference, picker[type='image']) nofulltext
 - og_site_name (string) = "Pr\\u00E9parons Ma Retraite" nofulltext
 - og_type (string) = "website" nofulltext
 
//////////////////////////////////////////////////////////////////////////
// PAGE EXTENSIONS ///////////////////////////////////////////////////////
//////////////////////////////////////////////////////////////////////////

//definition du marqueur page pour les rubriques, les forums, et la page modèle pour les questions
[jmix:ciMarkerPageption] mixin
 extends=jnt:page
 itemtype=options
 - isRubric (boolean) nofulltext
 - isThematic (boolean) nofulltext
 - isForum (boolean) nofulltext
 - isDummyQuestion (boolean) nofulltext
// facebook metadata to be picked on sharing for instance an article (only
// present here currently but could be extended)
[jmix:ciFbPageMetadata] mixin
  extends=jnt:page
  itemtype=options
 - og_title (string) nofulltext
 - og_url (string) nofulltext
 - og_description (string) nofulltext
 - og_type (string) nofulltext hidden indexed=no
 - og_site_name (string) nofulltext hidden indexed=no
 - og_image0 (weakreference, picker[type='image']) nofulltext
 - og_image1 (weakreference, picker[type='image']) nofulltext
 - og_image2 (weakreference, picker[type='image']) nofulltext

[jmix:ciPageExtensions] > jmix:ciViewCountable, jmix:ciSearchable mixin
 extends=jnt:page
 itemtype=options
 - shortTitle (string) boost=0.5
 - description (string) boost=0.5
 - logo (weakreference, picker[type='file']) indexed=no
 - image (weakreference, picker[type='file']) indexed=no
 - sliderImage (weakreference, picker[type='file']) indexed=no
 - imageSlider (weakreference, picker[type='file']) hidden indexed=no

[jmix:ciPageProperties] mixin
 extends=jnt:page
 itemtype=options
 - title (string)

