[jnt:ciToolsStats] > jnt:content, jmix:ciConfigComponents orderable
 - jcr:title (string) = 'Statistiques' nofulltext mandatory indexed=no
 
[jnt:ciToolsNotifs] > jnt:content, jmix:ciConfigComponents orderable
 - jcr:title (string) = 'Gestion notifications' nofulltext mandatory indexed=no

[jnt:ciToolsPatches] > jnt:content, jmix:ciConfigComponents orderable
 - jcr:title (string) = 'Gestion patches' nofulltext mandatory indexed=no


